.PHONY: clean All

All:
	@echo "----------Building project:[ UniquePointers - Debug ]----------"
	@cd "UniquePointers" && "$(MAKE)" -f  "UniquePointers.mk"
clean:
	@echo "----------Cleaning project:[ UniquePointers - Debug ]----------"
	@cd "UniquePointers" && "$(MAKE)" -f  "UniquePointers.mk" clean
